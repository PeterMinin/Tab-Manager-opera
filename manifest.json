{
	"name" : "Easy Tab Manager",
	"description" : "Advance tab management tool.",
	"icons" : {
		"128" : "img/icon_128.png"
	},
	"developer" : {
		"name" : "mubaidr",
		"url" : "http://my.opera.com/mubaidr/blog/"
	},
	"homepage_url" : "http://my.opera.com/mubaidr/blog/2013/08/28/easy-tab-manager",
	"browser_action" : {
		"default_title" : "Manage Tabs",
		"default_popup" : "html/popup.html",
		"default_icon" : "img/icon_enable.png"
	},
	"version" : "1.0",
	"manifest_version" : 2,
	"background" : {
		"scripts" : ["js/background.js"],
		"persistent" : false
	},
	"options_page" : "html/option.html",
	"permissions" : ["tabs", "alarms"],
	"offline_enabled" : true
}
