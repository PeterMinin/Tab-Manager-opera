{
	"name" : "Tab Manager +",
	"description" : "Advance tab management tool.",
	"icons" : {
		"128" : "img/icon_enable.png"
	},
	"developer" : {
		"name" : "mubaidr",
		"url" : "http://my.opera.com/mubaidr/blog/"
	},
	"homepage_url" : "http://my.opera.com/mubaidr/blog/",
	"browser_action" : {
		"default_title" : "Manage Tabs",
		"default_popup" : "html/popup.html",
		"default_icon" : "img/icon_enable.png"
	},
	"version" : "0.1",
	"manifest_version" : 2,
	"background" : {
		"scripts" : ["js/background.js"],
		"persistent" : true
	},
	"options_page" : "html/option.html",
	"permissions" : ["tabs"],
	"offline_enabled" : true
}
